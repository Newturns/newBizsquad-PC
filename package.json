{
  "name": "bizsquad",
  "version": "1.3.2",
  "author": {
    "name": "kimtaehwan",
    "email": "skyzard1@gmail.com",
    "url": "https://www.bizsquad.net"
  },
  "homepage": "https://ionicframework.com/",
  "scripts": {
    "dev": "nf start",
    "start": "ionic-app-scripts serve",
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "ionic:build": "ng build",
    "ionic:serve": "ionic-app-scripts serve",
    "publish": "electron-builder --mac --win --publish always",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "dist-all": "electron-builder -mw",
    "dist-win": "electron-builder --win",
    "sign-dev-electron-app": "codesign --deep --force --verbose --sign node_modules/electron-prebuilt-compile/node_modules/electron/dist/Electron.app",
    "postinstall": "npm run sign-dev-electron-app"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/fire": "^5.2.3",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "@ionic-native/core": "^5.18.0",
    "@ionic-native/splash-screen": "^5.18.0",
    "@ionic-native/status-bar": "^5.18.0",
    "@ionic/angular": "^4.11.7",
    "@types/deep-equal": "^1.0.1",
    "bootstrap": "^4.4.1",
    "core-js": "^2.6.11",
    "deep-equal": "^1.1.1",
    "electron-context-menu": "^0.15.1",
    "electron-default-menu": "^1.0.1",
    "electron-json-storage": "^4.1.8",
    "electron-log": "^4.0.0",
    "electron-updater": "^4.2.0",
    "electron-window-state": "^5.0.3",
    "firebase": "^7.6.0",
    "rxjs": "~6.5.1",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/architect": "~0.801.2",
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular-devkit/core": "~8.1.2",
    "@angular-devkit/schematics": "~8.1.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@ionic/angular-toolkit": "^2.1.1",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "electron": "^7.1.2",
    "electron-builder": "^22.2.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3"
  },
  "description": "An Ionic project",
  "main": "electron/main.js",
  "config": {
    "ionic_source_map_type": "webpack",
    "ionic_webpack": "./config/webpack.config.js"
  },
  "build": {
    "publish": [
      {
        "provider": "github",
        "owner": "Newturns",
        "repo": "newBizsquad-PC"
      }
    ],
    "appId": "com.bizsquad.ionic",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "build/logo512.png"
    },
    "win": {
      "verifyUpdateCodeSignature": false,
      "target": [
        "nsis"
      ],
      "icon": "build/logo512.png"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "language": 1032,
      "shortcutName": "Bizsquad"
    },
    "asar": true,
    "files": [
      "www/**/*",
      "electron/*"
    ]
  }
}
